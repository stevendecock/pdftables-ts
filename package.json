{
  "name": "pdftables-ts",
  "version": "0.2.1",
  "description": "TypeScript library to extract tables from PDFs using glyph coordinates, preserving empty cells.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/src/index.js",
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "run:pdf": "npm run build && node dist/test/manual-run.js",
    "run:glyphs": "npm run build && node dist/test/list-glyphs.js",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "pdf",
    "table",
    "extract",
    "typescript"
  ],
  "author": "Steven Decock",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stevendecock/pdftables-ts.git"
  },
  "bugs": {
    "url": "https://github.com/stevendecock/pdftables-ts/issues"
  },
  "homepage": "https://github.com/stevendecock/pdftables-ts#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "pdfjs-dist": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0"
  }
}
